<?php
function onEnd()
{
    $this['per_page_limit'] = 6;
    $this['experiences'] = san\San\Models\Destinations::where('status', '1')->orderBy('title', 'ASC')->get();
    $this['packages'] = san\San\Models\Packages::where('on_holiday_page', '1')->orderBy('sort_order', 'ASC')->take(12)->get();
}
