   	<script>
   	        $("#pills-holiday-tab").click(function () {
   	            $('#home-bg-images').css({ 'background-image': 'url(' + $("#holidayimg").val() + ')' });
   	            $('#tabedcontent').html($("#holidaycotent").val());
   	            $(".home-banner-link").prop("href", $("#holidaylink").val());
