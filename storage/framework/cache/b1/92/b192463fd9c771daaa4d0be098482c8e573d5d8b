        $('.enquire').click(function() {
            var dataValue = $(this).parent().data('title');
            setTimeout(function() {
                $('#area_of_interest').val(dataValue);
                if(dataValue == "Ground transportation"){
                    $('.onlyforgp').show();
                    $('.onlyforcity').hide();
                }else{
                    $('.onlyforgp').hide();
                    $('.onlyforcity').show();
                }
