                    $('#carrosalwrapper .item').eq(i).find('span').html(img[i].name);
                }
            } else {
                for (i = 0 + cr.cd; i < 4 + cr.cd; i++) {
                    $('#carouselround').prepend('<div class="item" onclick="cr.click(this)" ><div class="title"><span></span><em></em></div></div>');
                    cr.setdata(0, i);
                }
            }
            {% endif %}
        },
    btnclick: function(a, ele) {
        cr.cl = a;
        $('.btn-group.themes_button').find('.btn').removeClass('active');
        $(ele).addClass('active');
        cr.int();
        setTimeout(function () {
            cr.set();
        }, 300);
    },
    click: function(a) {
        i = $('#carrosalwrapper .item').index(a);
        if (i == 0 || i == 2) {
            corrosalmove(i);
        }
    },
    setdata: function(ele, i) {
        img = cr.img[cr.cl][i];
        if (img) $('#carrosalwrapper .item').eq(ele).attr('id', i).css({ 'background-image': 'url(' + img.exp_image }).find('span').html(img.name);
    },
    set: function () {
        cr.w = $('#carrosalwrapper #carouselround').width();
