                    <script>
                        function expslide(type, ele) {
                            toele = "";
                            if (ele) {
                                toele = $(ele).closest('.experienc-set');
                                if ($(toele).length && toele.hasClass('experienc-set')) {
                                    if (!$(toele).hasClass('open')) {
                                        $('.experienc-set').removeClass('current');
                                        $('.experienc-set').removeClass('open');
                                        $(toele).addClass('active');
                                        $(toele).addClass('open');
                                        $(toele).addClass('current');
                                    } else {
                                        $(toele).removeClass('open');
                                        $(toele).removeClass('current');
                                    }
                                }
                            } else {
                                $('.experienc-set').each(function () {
                                    if (!toele) {
                                        ele = $(this);
                                        if (ele.hasClass('current')) {
                                            if (ele.hasClass('active') && !ele.hasClass('open')) {
                                                toele = ele;
