        <script>
            function expslide(type, ele) {
                toele = "";
                $('.experienc-set').each(function () {
                    if (!toele) {
                        ele = $(this);
                        if (ele.hasClass('current')) {
                            if (ele.hasClass('active') && !ele.hasClass('current')) {
                                toele = ele;
                            } else {
                                if (type == 1) {
                                    toele = ele.next();
                                } else {
                                    toele = ele.prev();
                                }
                            }
                        }
                        if ($(toele).length && toele.hasClass('experienc-set')) {
                            $('.experienc-set').removeClass('current');
                            $(toele).addClass('active');
                            $(toele).addClass('current');
                        }
                    }
                    index = $('.experienc-set').index(toele);
                    if ($('.experienc-set').length == index + 1) {
                        $('.morebar').addClass('d-none');
                    } else {
                        $('.morebar').removeClass('d-none');
                    }

                })
            }
        </script>
        {% endif %}


    </div>
