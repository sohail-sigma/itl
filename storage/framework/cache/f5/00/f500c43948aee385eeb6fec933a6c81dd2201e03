        $('#carrosalwrapper .item').eq(2).css({ width: cr.w * .6, left: cr.w - (cr.w * .6), top: cr.w * .05, transform: 'scale(.8)' });
        $('#carrosalwrapper .item').eq(3).css({ width: cr.w * .6, left: cr.w * .2, top: cr.w * .05, transform: 'scale(1)' });
