<script>
    var dp = {
        baseurl: '{{url("/")}}',
        current: 0,
        data: {{ packages| json_encode | raw }},
    put: function() {
        data = dp.data[dp.current];
        ele = $('.destination-packages');
        w = ele.find('.section').offset().left;
        ele.find('h2').html(data.name);
        ele.find('p').html(data.brief);
        ele.find('h5').html(data.days + ' Days From');
        ele.find('h3').html(data.price);
        ele.find('a.more').attr('href', dp.baseurl + 'package/' + data.slug);
        $('.leftwhite').css({ width: w, left: -w });
        ele.css({ 'background-image': 'url(' + data.image + ')' });
    },
    click: function(a) {
        if (a) {
            dp.current++;
        } else {
            dp.current++;
        }
        if (dp.current > dp.data.length - 1) {
            dp.current = 0;
        } else if (dp.current < 0) {
            dp.current = dp.data.length - 1;
        }

        dp.put();
    }
    }
    $(document).ready(function () {
        dp.put();
    })
</script>
