use San\San\Models\Countries;
use San\San\Models\Packages;
use San\San\Models\Themes;
function onEnd(){
    if((!@$_GET['preview'] || !Session::get('_token')) && $this->pageDetails->record){
        $page=$this->pageDetails->record->active(1)->where('slug',$this->pageDetails->record['slug'])->get();
        if(!count($page)){
            $this->setStatusCode(404);
            return $this->controller->run('404');
        }
    }
    if($metadata=$this->pageDetails->record['meta_title']) $this->page->meta_title = $metadata;
    if($metadata=$this->pageDetails->record['meta_desc']) $this->page->meta_description = $metadata;
    if($metadata=$this->pageDetails->record['meta_image']) $this->page->og_image = $metadata->getThumb(1200,630);
    $themesid = [];
    $countiresid=[];
    $this['exps']=[];
    $exps=[];
    $this['slug'] = Request::segment(2); 
    $experiences=[];
    $packages=[];
    foreach ($this->experiencesList->records as $k=>$record){
        if($record['slug']==$this['slug']){
            $this['cd']=$k;
        }
        $experiences[$k]=[
            'sub_title'=>$record['sub_title'],
            'name'=>$record['title'],
            'description'=>$record['content'],
            'image'=>$record['exp_image']?$record['exp_image']->getThumb(540,320,'crop'):"",
            'subs'=>[]
        ];
        $countries=Countries::select('id','title','brief','slug')->where('destination_id',$record['id'])->where('status',1)->get();
        foreach($countries as $country){
            $countiresid[$country->id] = $country->title;
            $experiences[$k]['subs'][]=[
                'name'=>$country->title,
                'brief'=>substr(strip_tags($country->brief), 0, 110),
                'id'=>$country->id,
                'slug'=>$country->slug,
                'image'=>$country->exp_banner?$country->exp_banner->getThumb(200,200,'crop'):""
            ];
        }
    }
    if ($this['slug'] && $countiresid) {
            $themesid = [];
            foreach($countiresid as $countryId){
                $packages=Packages::select('theme_id')->where('countries', 'like', '%'.$countryId.'%')->where('status',1)->get();
                if ($packages) {
                    foreach ($packages as $package) {
                        $themesid[$package['theme_id']] = $package['theme_id'];
                    }
                }
            }
            
            if ($themesid) {
                $themes=Packages::whereIn('theme_id',$themesid)->where('status',1)->get();
                foreach ($themes as $theme) {
                    $exps[] = [
                        'title'=>$theme->title,
                        'image'=>$theme->exp_banner?$theme->exp_banner->getThumb(583,583,'crop'):"",
                        'content'=>$theme->content
                    ];
                }
            }
            
           $this['exps']= $exps;
    }
    $this['destination']=$experiences;

    $themes=[];
    foreach ($this->themesList->records as $k=>$record){
        $themes[$k]=[
            'name'=>$record['title'],
            'sub_title'=>$record['sub_title'],
            'description'=>$record['content'],
            'image'=>$record['exp_banner']? $record['exp_banner']->getThumb(540,320,'crop'):"",
            'video'=>$record['video'],
            'subs'=>[]
        ];
        $packages=Packages::select('id','title','days','night','price','slug')->where('theme_id',$record['id'])->where('status',1)->get();
        foreach($packages as $item){
            $themes[$k]['subs'][]=[
                'name'=>$item->title,
                'nights'=>$item->night,
                'price'=>$item->price,
                'days'=>$item->days,
                'brief'=>substr(strip_tags($item->brief), 0, 110),
                'id'=>$item->id,
                'slug'=>$item->slug,
                'image'=>$item->exp_banner? $item->exp_banner->getThumb(200,200,'crop'):""
            ];
        }
    }
    $this['themes']=$themes;

    foreach ($this->packagesList->records as $k=>$record){
        $packages[$k]=[
            'name'=>$record['title'],
            'brief'=>$record['brief'],
            'days'=>$record['days'],
            'price'=>$record['price'],
            'slug'=>$record['slug'],
            'subs'=>[]
        ];
    }
    $this['packages']=$packages;
